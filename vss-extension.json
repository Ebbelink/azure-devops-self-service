{
  "manifestVersion": 1,
  "id": "access-control-self-service",
  "version": "0.1.89",
  "name": "Access Control self service",
  "publisher": "StarttoScaleIT",
  "categories": [
    "Azure Repos"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "src/images/logo.png"
  },
  "description": "A access control self service portal that provides an out of the box team, group, access setup and immediately provides self service for the end users to do this themselves",
  "tags": [
    "self service",
    "access control",
    "authorization",
    "rbac",
    "role based access control"
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/Ebbelink/azure-devops-self-service"
  },
  "content": {
    // "details": {
    //   "path": "access-control-extension/overview.md"
    // },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://www.starttoscaleit.nl/azure-devops/help/getting-started"
    },
    "learn": {
      "uri": "https://www.starttoscaleit.nl/azure-devops/features"
    },
    "support": {
      "uri": "https://www.starttoscaleit.nl/azure-devops/help"
    }
  },
  "branding": {
    "color": "rgb(34, 34, 34)",
    "theme": "dark"
  },
  "scopes": [
    "vso.graph_manage",
    "vso.project_manage"
  ],
  "contributions": [
    {
      "id": "self-service-hub-group",
      "type": "ms.vss-web.hub-group",
      "description": "Adds a Self Service hub group at the project level",
      "targets": [
        "ms.vss-web.project-hub-groups-collection"
      ],
      "properties": {
        "name": "Self Service",
        "order": 100,
        "icon": "asset://images/self-service-logo-green.png"
      }
    },
    {
      "id": "access-control-hub",
      "type": "ms.vss-web.hub",
      "description": "Adds a 'Access Control' hub to the Self Service hub group.",
      "targets": [
        ".self-service-hub-group"
      ],
      "properties": {
        "name": "Hello",
        "order": 99,
        "uri": "access-control-extension.html",
        "icon": "asset://images/logo.png"
      }
    }
  ],
  "files": [
    // {
    //   "path": "node_modules/azure-devops-extension-sdk",
    //   "addressable": true,
    //   "packagePath": "dist/azure-devops-extension-sdk"
    // },
    {
      "path": "src/access-control-extension.html",
      "addressable": true,
      "packagePath": "access-control-extension.html"
    },
    {
      "path": "src/images",
      "addressable": true,
      "packagePath": "images"
    },
    {
      "path": "dist/",
      "addressable": true
    }
  ],
  "public": false
}